{
  "version": 2,
  "endpoints": [
    {
    "method": "GET",
    "endpoint": "/auth/realms/note",
    "backend": [
      {
        "host": [
          "http://note_auth:8080"
        ],
        "url_pattern": "/auth/realms/note"
      }
    ]
},
{
    "method": "POST",
    "endpoint": "/token",
    "backend": [
      {
        "host": "http://note_auth:8080",
        "url_pattern": "/auth/realms/note/protocol/openid-connect/token"
      }
    ]
},
    {
  "endpoint": "/v1/bookmark/{id}",
  "backend": [
    {
      "url_pattern": "/v1/bookmark/{id}",
      "host": [
          "http://host.docker.internal:8081"
        ]
    }
  ],
  "extra_config": {
  "github.com/devopsfaith/krakend-jose/validator": {
    "disable_jwk_security": true,
    "alg": "RS256",
    "roles_key_is_nested": true,
    "roles_key": "realm_access.roles",
    "roles": [
      "default-roles-note"
    ],
    "jwk-url": "http://note_auth:8080/auth/realms/note/protocol/openid-connect/certs"
  }
}
}
  ]
}